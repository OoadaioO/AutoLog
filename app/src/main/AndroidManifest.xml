<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cc.kaipao.dongjia.jetpack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cc.kaipao.dongjia.jetpack.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="cc.kaipao.dongjia.jetpack.SecondActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="cc.kaipao.dongjia.jetpack.MainActivity" />
        </activity>

    </application>

</manifest>